@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix ex: <http://example.org/>.
@prefix dbpedia: <http://dbpedia.org/resource/>.

ex:Univeristy a rdfs:Class;
    rdfs:label "University";
    .
dbpedia:Concordia_University a ex:Univeristy;
        rdfs:label "Concordia University";
        .

ex:Course a rdfs:Class;
    rdfs:label "Course";
    .

ex:offersCourse a rdfs:Property;
    rdfs:label "offersCourse";
    rdfs:domain ex:Univeristy;
    rdfs:range ex:Course;
    .

ex:hasName a rdfs:Property;
    rdfs:label "hasName";
    .

ex:Intelligent_Systems a ex:Course;
    ex:hasName "Intelligent System";
    .    